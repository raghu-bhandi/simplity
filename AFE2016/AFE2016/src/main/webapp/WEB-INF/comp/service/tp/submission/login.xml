<?xml version="1.0" encoding="UTF-8"?>
<service
	dbAccessType="none"
	description="Login Service"
	executeInBackground="true"
	moduleName="submission"
	name="login"
	xmlns="http://www.simplity.org/schema">
	<inputData>
		<inputFields>
			<inputField dataType="text" name="_userId"/>
			<inputField dataType="text" name="_userToken"/>			
		</inputFields>
	</inputData>
	<outputData fieldNames="userId">		
	</outputData>
	<actions>
		<log names="_userId"/>
		<setValue fieldName="userId" fieldValue="trustme"></setValue>		
	</actions>
</service>