<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<service
	xmlns="http://www.simplity.org/schema"
	moduleName="ldap"
	name="ldapAuth"
	dbAccessType="none">
	<inputData>
		<inputFields>
			<inputField
				dataType="_text"
				name="_userId" />
			<inputField
				dataType="_text"
				name="_userToken" />
		</inputFields>
	</inputData>
	<actions>
		<ldapAuthenticate
			credentials="$_userToken"
			principal="$_userId"></ldapAuthenticate>
	</actions>
	<outputData fieldNames="_userId">
	</outputData>
</service>