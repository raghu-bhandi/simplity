<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<service
	xmlns="http://www.simplity.org/schema"
	moduleName="ldap"
	name="ldapLookupMultiAttr"
	dbAccessType="none">
	<inputData>
		<inputFields>
			<inputField
				dataType="_text"
				name="objectId" />
			<inputField
				dataType="_text"
				name="outputDataSheetName" />
		</inputFields>
	</inputData>
	<actions>
		<ldapLookup>
			<multiAttrs objectId="$objectId" outputDataSheetName="$outputDataSheetName">
				<attrIDs>surname</attrIDs>
			</multiAttrs>
		</ldapLookup>
	</actions>
	<outputData dataSheets="$outputDataSheetName"></outputData>
</service>