<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<service
	xmlns="http://www.simplity.org/schema"
	moduleName="ldap"
	name="ldapLookupExists"
	dbAccessType="none">
	<inputData>
		<inputFields>
			<inputField
				dataType="_text"
				name="objectId" />
			<inputField
				dataType="_text"
				name="fieldname" />
		</inputFields>
	</inputData>
	<actions>
		<ldapLookup>
			<exists objectId="$objectId" fieldName="$fieldname"></exists>
		</ldapLookup>
	</actions>
	<outputData fieldNames="$fieldname"></outputData>
</service>