<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<service
	xmlns="http://www.simplity.org/schema"
	moduleName="ldap"
	name="ldapLookupSingleAttr"
	dbAccessType="none">
	<inputData>
		<inputFields>
			<inputField
				dataType="_text"
				name="objectId" />
			<inputField
				dataType="_text"
				name="attrName" />
		</inputFields>
	</inputData>
	<actions>
		<ldapLookup>
			<singleAttr
				objectId="$objectId"
				attrName="$attrName"
				fieldName="surname"></singleAttr>
		</ldapLookup>
	</actions>
	<outputData fieldNames="surname"></outputData>
</service>