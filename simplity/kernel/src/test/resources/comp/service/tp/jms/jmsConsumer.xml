	<actions>
		<createSheet sheetName="commentSheet" recordName="comments" />
		<log actionName="emptysheet" names="commentSheet" />
		<jmsConsumer waitForMessage="false" consumeAll="true">
			<requestJmsObject queueName="jms/Queue01" recordName="comments"
				extractAll="true" />
