<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<service
	xmlns="http://www.simplity.org/schema"
	moduleName="ldap"
	name="ldapLookup"
	dbAccessType="none">
	<inputData>
		<inputFields>
			<inputField
				dataType="_text"
				name="objectId" />
			<inputField
				dataType="_text"
				name="attrName" />
		</inputFields>
	</inputData>
	<actions>
		<ldapLookup
			objectId="$objectId"
			attrName="$attrName"
			fieldName="ldapLookup"></ldapLookup>
	</actions>
	<outputData fieldNames="ldapLookup"></outputData>
</service>