<?xml version="1.0" encoding="UTF-8"?>
<sql
	name="selectStudent"
	moduleName="example"
	sqlType="multiSelect"
	xmlns="http://www.simplity.org/schema">
	<inputParameters>
		<sqlParameter
			dataType="_number"
			name="id2" />
	</inputParameters>
	<preparedStatement>SELECT * FROM TESTSCHEMA.STUDENTS WHERE rollNumber =?
	</preparedStatement>
	<outputParameters>
		<sqlParameter dataType="_number" name="rollNumber"/>
		<sqlParameter
			dataType="_text"
			name="name" />
		<sqlParameter
			dataType="_text"
			name="schoolName" />
	</outputParameters>
</sql>