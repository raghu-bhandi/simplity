<?xml version="1.0" encoding="UTF-8"?>
<sql name="Students" sqlType="singleSelect" xmlns="http://www.simplity.org/schema" 
	preparedStatement="Select name From TESTSCHEMA.Students where rollNumber  = ?"
	moduleName="example" >
	<inputParameters>
		<sqlParameter dataType="id" name="rollNumber "
			defaultValue="2" />
	</inputParameters>
	<outputParameters>
		<sqlParameter dataType="text" name="name" />
	</outputParameters>
</sql>
