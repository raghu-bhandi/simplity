<?xml version="1.0" encoding="UTF-8"?>
<sql name="Employees" sqlType="singleSelect"
    xmlns="http://www.simplity.org/schema"
	preparedStatement="Select lastName From Employees where employeeNumber = ?"
	moduleName="example">
	<inputParameters>
		<sqlParameter dataType="id" name="employeeNumber"
			defaultValue="1056" />
	</inputParameters>
	<outputParameters>
		<sqlParameter dataType="text" name="lastName" />
	</outputParameters>
</sql>
