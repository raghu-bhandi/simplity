<?xml version="1.0" encoding="UTF-8"?>
<record name="registration" 
		moduleName="smtp"
		tableName="registration"
		keyToBeGenerated="true"
		recordType="storage"
		xmlns="http://www.simplity.org/schema">
	<fields>
		<field name="registrationId" dataType="_number" columnName="registrationId" fieldType="primaryKey" />
		<field name="submitterId" dataType="_text" columnName="submitterId" isNullable="true"/>
		<field name="domain" dataType="_text" columnName="domain" isNullable="true"/>
		<field name="application" dataType="_text" columnName="application" isNullable="true"/>
		<field name="mailid" dataType="_text" columnName="mailid" isNullable="true"/>
		<field name="apikey" dataType="_text" columnName="apikey" isNullable="true" />		
	</fields>
	
</record>
