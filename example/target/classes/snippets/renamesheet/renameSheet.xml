<!-- File name: renamesheet.xml under comp/services/tp/tutorial -->

<service 	xmlns="http://www.simplity.org/schema"
	name="renameSheet"
	moduleName="tutorial"
	dbAccessType="none"
	description="This is the customary example that every one is used to..">
	<outputData fieldNames="testValue">
		<outputRecords>
			<outputRecord  sheetName="newweekendBoxOffice" />
		</outputRecords>
	</outputData>
	<actions>
	<logic actionName="1" className="org.simplity.tutorial.CustomLogicAction" />
	<renameSheet newSheetName="newweekendBoxOffice" sheetName="weekendBoxOffice" actionName="renameSheetAction"/>
	<setValue fieldName="testValue" fieldValue="$renameSheetActionResult" executeOnCondition="renameSheetActionResult"></setValue>
	</actions>
</service>
