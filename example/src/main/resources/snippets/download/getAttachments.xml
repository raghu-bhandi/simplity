<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<service
	xmlns="http://www.simplity.org/schema"
	name="getAttachments"
	moduleName="tutorial"
	dbAccessType="none"
	description="down load a set of files for demonstration purposes"
>
	<actions>
		<createSheet
			sheetName="files"
			data="key,name,mime;getstarted,temp,temp" />
	</actions>
	<outputData fieldNames="rootFolder" attachmentColumns="key">
		<outputRecords>
			<outputRecord sheetName="files"/>
		</outputRecords>
	</outputData>
</service>