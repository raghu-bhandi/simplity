<!-- File name: rowExists.xml under comp/services/tp/tutorial -->
<service xmlns="http://www.simplity.org/schema" name="rowExists"
	moduleName="tutorial" dbAccessType="readOnly" description="Test actions row exists" >
	<actions>
		<setValue fieldName="employeeNumber" fieldValue="1056"></setValue>
		<rowExists recordName="birt.Employees" actionName="rowExistsAction"
			description="Check if row exists"  fieldName="employeeNumber" />
			<setValue fieldName="testValue" fieldValue="$rowExistsActionResult" executeOnCondition="rowExistsActionResult"></setValue>
	</actions>
	<outputData fieldNames="testValue"/>
</service>
