<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<record xmlns="http://www.simplity.org/schema" 
	name="customers"
	moduleName="birt" 
	recordType="storage"
	okToSelectAll="true"
	keyToBeGenerated="true"
	>
	<fields>
		<field name="customerNumber" dataType="_number" fieldType="primaryKey" />
		<field name="customerName" dataType="_text" />
		<field name="contactLastName" dataType="_text" />
		<field name="contactFirstName" dataType="_text" />
		<field name="phone" dataType="_text" />
		<field name="addressLine1" dataType="_text" />
		<field name="addressLine2" dataType="_text"  isNullable="true" />
		<field name="city" dataType="_text" />
		<field name="state" dataType="_text"  isNullable="true" />
		<field name="postalCode" dataType="_text"  isNullable="true" />
		<field name="country" dataType="_text" />
		<field name="salesRepEmployeeNumber" dataType="_number"  isNullable="true"/>
		<field name="creditLimit" dataType="_decimal" isNullable="true"/>
	</fields>
</record>
