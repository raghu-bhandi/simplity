<?xml version="1.0" encoding="UTF-8"?>
<service
	dbAccessType="readOnly"
	name="callFilterRec"
	xmlns="http://www.simplity.org/schema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"	
>
   <actions>   
    <filter filterRecordName="agents" outputSheetName="agents">
    	<childRecords>
    		<relatedRecord sheetName="customers" recordName="customer"/>
    	</childRecords>
    </filter>
    <addColumn sheetName="agents" columnName="mixedCol" columnValueType="text" columnValueExpression="concat(agents.agentCode,agents.agentName)"/>
    <log names="agents"/>
   </actions>
   <outputData dataSheets="agents"></outputData>
</service>