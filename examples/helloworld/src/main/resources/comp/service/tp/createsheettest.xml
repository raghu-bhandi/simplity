<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<service
	xmlns="http://www.simplity.org/schema"
	name="createsheettest"
	dbAccessType="none"
>
	<actions>
		<createSheet
			sheetName="abc"
			recordName="hello" />
		<setValue
			fieldName="id"
			fieldValue="1"
		></setValue>
		<setValue
			fieldName="greeting"
			fieldValue="hello"
		></setValue>
		<addRow sheetName="abc" actionName="addrow"/>
	</actions>
	<outputData dataSheets="abc"></outputData>
</service>