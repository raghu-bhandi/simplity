<?xml version="1.0" encoding="UTF-8"?>
<sql
	name="selectStudent"
	sqlType="multiSelect"
	xmlns="http://www.simplity.org/schema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.simplity.org/schema http://www.simplity.org/schema "
>
	<inputParameters>
		<sqlParameter
			dataType="_number"
			name="id2" />
	</inputParameters>
	<preparedStatement>SELECT * FROM STUDENT WHERE id =?
	</preparedStatement>
	<outputParameters>
		<sqlParameter dataType="_number" name="id"/>
		<sqlParameter
			dataType="_text"
			name="name" />
		<sqlParameter
			dataType="_text"
			name="address" />
	</outputParameters>
</sql>