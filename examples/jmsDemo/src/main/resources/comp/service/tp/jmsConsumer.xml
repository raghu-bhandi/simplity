<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<service
	xmlns="http://www.simplity.org/schema"
	name="jmsConsumer"
	dbAccessType="none"
	jmsUsage="notManaged">
	<actions>
		<jmsConsumer waitForMessage="false" consumeAll="true">
			<requestQueue queueName="jms/Queue01" extractAll="true"/>
			<actions>
				<log names="message"/>
			</actions>
		</jmsConsumer>
	</actions>
	<outputData fieldNames="message"></outputData>
</service>